{
  "extends": "./.svelte-kit/tsconfig.json",
  "compilerOptions": {
	"target": "esnext",
	"module": "esnext",
	"strict": false,
	"esModuleInterop": true,
	"skipLibCheck": true,
	"forceConsistentCasingInFileNames": true,
	"allowJs": true,
	"checkJs": true,
	"moduleResolution": "bundler"
  },
  "include": ["./**/*.ts", "./**/*.js", "./**/*.svelte"],
  "exclude": ["node_modules"]
}